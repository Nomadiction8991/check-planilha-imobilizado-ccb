# === PHP project ignore rules ===
# Dependencies (installed via Composer)
/vendor/

# Environment / secrets
.env
.env.*

# IDE / editor
.vscode/
.idea/
*.code-workspace

# OS files
.DS_Store
Thumbs.db

# Caches / logs
*.log
cache/
storage/
/tmp/

# Only keep GitHub workflows, ignore other .github content
.github/*
.github/instructions/*
!.github/workflows/
!.github/workflows/**

# Node (if ever used)
/node_modules/

# Misc
*.bak
*.swp
